<h1> Client Registartion <h1>

<%= form_with model: @client, url: new_registration_path do |form|%>
    <div class="mb-3">
    <%= form.text_field :name, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.text_field :parent_name, class="form-contro;" %>
    </div>
    <div class="mb-3">
    <%= form.text_field :insurance, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.text_field :address, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.text_field :phone_number, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.text_field :email, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.password_field :password, class="form-control" %>
    </div>
    <div class="mb-3">
    <%= form.password_field :password_confirmation, class="form-control" %>
    </div>
<% end %>