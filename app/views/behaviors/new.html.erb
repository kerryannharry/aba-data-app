<h1> Behavior: </h1>

<%= form_with(model: @behavior, url: new_behavior_path) do |form|%>
    <div class="mb-3">
    <%= form.label :name, "Behavior Name:" %>
    <%= form.text_field :name, class: "form-control" %>
    </div>
    <div class="mb-3">
    <%= form.label :name, "Behavior Frequency:" %>
    <%= form.integer_field :frequency, class: "form-control" %>
    </div>
    <div class="mb-3">
    <%= form.label :name, "Behavior duration:" %>
    <%= form.time_field :duration, class: "form-control" %>
    </div>